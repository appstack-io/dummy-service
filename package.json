{
  "name": "dummy-service",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/appstack-io/dummy-service",
  "scripts": {
    "build": "nest build",
    "generate-interfaces": "ts-node generate-interfaces.ts",
    "proto:client": "cd proto && ../node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./client --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false --proto_path=. combined.proto && cd .. && cp proto/client/combined.ts src/libs/client/index.ts",
    "proto:beapi": "cd proto && node generateBeapi.js && eslint --fix beapi && cp -r beapi/* ../../expo/resources/beapi/services/dummy-service",
    "proto:build": "npm run proto:combine && npm run proto:compile && npm run proto:interfaces",
    "proto:build:client": "npm run proto:build && npm run proto:client && npm run proto:beapi",
    "start:ts": "ts-node src/main/index.ts",
    "setup:ts": "bash infra/setup.sh && ts-node infra/setup/runSetup.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest -i",
    "test:e2e": "E2E=1 SERVICE_HOST=service jest --runInBand"
  },
  "dependencies": {
    "@appstack-io/arangodb": "^0.6.0",
    "@appstack-io/authnz": "^0.1.0",
    "@appstack-io/client": "^0.1.1",
    "@appstack-io/config": "^0.1.0",
    "@appstack-io/exceptions": "^0.1.1",
    "@appstack-io/gateway": "^0.1.0",
    "@appstack-io/health": "^0.0.3",
    "@appstack-io/identity": "^0.0.3",
    "@appstack-io/integrity": "^0.0.2",
    "@appstack-io/limits": "^0.0.4",
    "@appstack-io/main": "^0.0.11",
    "@appstack-io/messaging": "^0.0.2",
    "@appstack-io/mq": "^0.1.0",
    "@appstack-io/permissions": "^0.0.0",
    "@appstack-io/pubsub": "^0.0.2",
    "@appstack-io/redis": "^0.3.2",
    "@appstack-io/social": "^0.0.1",
    "@appstack-io/visibility": "^0.1.0",
    "@appstack-io/workers": "^0.0.1"
  },
