    "axios": "^1.4.0",
    "detect-port": "^1.5.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "find-free-ports": "^3.1.1",
    "grpc-tools": "^1.12.4",
    "grpc-web": "^1.4.2",
